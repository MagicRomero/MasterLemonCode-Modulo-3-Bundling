{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-typescript",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "safari >= 7"
                    ],
                    "node": "6.10"
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}